{
  "name": "chaban",
  "version": "1.2.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "core": "turbo run build --filter=core",
    "mobile": "turbo run dev --filter=mobile",
    "app": "turbo run dev --filter=app",
    "dev": "turbo run dev --parallel",
    "generate": "turbo run generate",
    "seed": "turbo run seed --parallel",
    "kill": "turbo run kill --parallel",
    "docker": "turbo run docker --parallel",
    "test": "turbo run test --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.5"
}
