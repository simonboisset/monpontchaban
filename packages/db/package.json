{
  "name": "db",
  "version": "1.3.0",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && prisma generate",
    "dev": "prisma generate --watch",
    "seed": "prisma db seed",
    "drop": "node -r @swc-node/register ./src/drop.ts",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "reset": "prisma migrate reset --force",
    "version:major": "yarn version --new-version major --no-git-tag-version",
    "version:minor": "yarn version --new-version minor --no-git-tag-version",
    "version:patch": "yarn version --new-version patch --no-git-tag-version"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@swc/core": "^1.3.6",
    "@types/pg": "^8.6.5",
    "dotenv": "^16.0.3",
    "pg": "^8.8.0",
    "prisma": "^4.2.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1"
  },
  "prisma": {
    "seed": "node -r @swc-node/register ./prisma/seed.ts"
  }
}
