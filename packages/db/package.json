{
  "name": "@lezo-alert/db",
  "version": "1.4.8",
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn with-env builder && tsc",
    "build:dev": "yarn with-env builder --dev && tsc",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "yarn with-env prisma generate",
    "db:push": "yarn with-env prisma db push --skip-generate",
    "db:dev": "yarn with-env prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "studio": "yarn with-env prisma studio --port 5556",
    "seed": "prisma db seed",
    "with-env": "dotenv -e ../../.env --"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "builder": "*",
    "dotenv-cli": "^7.1.0",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
