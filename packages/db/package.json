{
  "name": "@lezo-alert/db",
  "version": "1.5.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "yarn with-env prisma generate",
    "db:push": "yarn with-env prisma db push --skip-generate",
    "db:dev": "yarn with-env prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "studio": "yarn with-env prisma studio --port 5556",
    "seed": "prisma db seed",
    "with-env": "dotenv -e ../../.env --"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "dotenv-cli": "^7.1.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
