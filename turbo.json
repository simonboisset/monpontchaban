{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**", ".netlify/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["generate", "^build"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "kill": {
      "cache": false
    },
    "docker": {
      "cache": false
    },
    "seed": {
      "cache": false
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "version:major": {
      "cache": false
    },
    "version:minor": {
      "cache": false
    },
    "version:patch": {
      "cache": false
    }
  }
}
