{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "db:generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:dev": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:deploy": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build:dev"],
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "outputs": ["dist/**", ".expo/**"]
    },
    "build:dev": {
      "dependsOn": ["^build:dev", "^db:generate"],
      "outputs": ["dist/**", ".expo/**"]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "kill": {
      "cache": false
    },
    "stop": {
      "cache": false
    },
    "push": {
      "cache": false
    },
    "docker": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "seed": {
      "dependsOn": ["^db:push"],
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db:generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "SESSION_SECRET",
    "JWT_SECRET",
    "RESEND_SECRET",
    "CRON_SECRET",
    "EXPO_ACCESS_TOKEN",
    "CHABAN_CHANNEL_ID"
  ]
}
