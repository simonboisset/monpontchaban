{
  "name": "app",
  "private": true,
  "sideEffects": false,
  "version": "1.2.2",
  "scripts": {
    "version:major": "yarn version --new-version major --no-git-tag-version",
    "version:minor": "yarn version --new-version minor --no-git-tag-version",
    "version:patch": "yarn version --new-version patch --no-git-tag-version",
    "build": "tailwindcss -o ./app/styles/tailwind.css && remix build && cp cron/update-cache.ts  .netlify/functions-internal/update-cache.ts && cp cron/send-notifications.ts  .netlify/functions-internal/send-notifications.ts",
    "dev": "concurrently \"tailwindcss -o ./app/styles/tailwind.css --watch\" \"cross-env NODE_ENV=development netlify dev\" \"tsc --noEmit --watch\"",
    "start": "cross-env NODE_ENV=production netlify dev",
    "story": "concurrently \"tailwindcss -o ./app/styles/tailwind.css --watch\" \"ladle serve\"",
    "kill": "docker stop $(docker ps -aq) && docker rm $(docker ps -aq)",
    "up": "yarn upgrade-interactive --latest",
    "setup": "prisma generate dev",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "reset": "prisma migrate reset --force",
    "docker": "docker-compose up -d",
    "seed": "prisma db seed",
    "ts": "tsc --noEmit",
    "test": "NODE_ENV=production yarn jest",
    "restart": "yarn kill && yarn docker && yarn setup && yarn dev"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0",
    "@prisma/client": "^4.4.0",
    "@remix-run/netlify": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "core": "*",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "expo-server-sdk": "^3.6.0",
    "ioredis": "^5.2.3",
    "node-fetch": "^3.2.10",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@swc-node/register": "^1.5.4",
    "@swc/core": "^1.3.6",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.1.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.10",
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node -r @swc-node/register ./prisma/seed.ts"
  }
}
