{
  "build": {
    "production": {
      "distribution": "store",
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "release": {
      "distribution": "internal",
      "channel": "dev",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./api-key.json",
        "track": "production"
      }
    }
  },
  "cli": {
    "version": ">= 0.53.1"
  }
}
