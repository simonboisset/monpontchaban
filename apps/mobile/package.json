{
  "name": "mobile",
  "main": "index.js",
  "version": "1.2.2",
  "scripts": {
    "version:major": "yarn version --new-version major --no-git-tag-version",
    "version:minor": "yarn version --new-version minor --no-git-tag-version",
    "version:patch": "yarn version --new-version patch --no-git-tag-version",
    "dev": "expo start",
    "test": "jest",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "up": "yarn upgrade-interactive --latest",
    "doctor": "expo-cli doctor --fix-dependencies",
    "apk": "eas build --profile release --platform android --no-wait --non-interactive",
    "deploy": "eas build --profile production --platform android --no-wait --auto-submit --non-interactive",
    "push": "eas update --auto"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.9",
    "core": "*",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "expo": "^46.0.7",
    "expo-application": "~4.2.2",
    "expo-constants": "~13.2.3",
    "expo-device": "~4.3.0",
    "expo-notifications": "~0.16.1",
    "expo-splash-screen": "~0.16.1",
    "expo-updates": "~0.14.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-safe-area-context": "4.3.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.18.9",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.0",
    "@types/react-native": "~0.69.5",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-jest": "^29.0.1",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "typescript": "~4.8.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "private": true
}
